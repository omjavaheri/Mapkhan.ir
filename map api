<!DOCTYPE html >
  <html >
   <head >
    <title >Mapkhan map</title >
    <meta name="viewport" content="initial-scale=1.0" >
    <meta charset="utf-8" >
    <style >
     /* Always set the map height explicitly to define the size of the div
     * element that contains the map. */
     #map {height: 100%;}
     /* Optional: Makes the sample page fill the window. */
     html, body {height: 100%;margin: 0;padding: 0;}
    </style >
  </head >
   <body >
    <div id="map" ></div >
    <script >
     var map;
     function initMap() {
      map = new Mapkhan.maps.Map(document.getElementById('map'), {
      center: { lat: -34.397, lng: 150.644 },
      zoom: 8
     });
    }
   </script >
<script src="http://api.mapkhan.ir/map%20JS%20file/JSBuild.ashx?callback=initMap" async defer></script>
   </body >
  </html > 
